#include <iostream>
#include <cstdio>
using namespace std;

int main() {
    
    for (int i = 0; i < 12; i++)
    {
        if (i==8)
        {
            cout << "eight" << endl;
        }

        
        else if (i==9)
        {
            cout << "nine" << endl;
        }
        
        else if (i >= 10)
        {
            if (i % 2 == 0)
            {
                cout << "even" << endl;
            }

            else 
            {
                cout << "odd";
            }
        }

        /*
        else
        {
            cout << "oops";
        }
        */

    }

    return 0;
}



    return 0;
}

